  <DataSets>
    <DataSet Name="Products">
      <Query>
        <DataSourceName>dtsrc_products</DataSourceName>
        <CommandText>SELECT     tblClient.ClientID, tblClient.ClientName, tblProductMaster.Division_MDB, tblDivision.DivisionID, tblDivision.DivisionName, tblDivision.BusinessUnit, 
                      tblBulkOrderItem.SupplyID AS Supplier, tblBulkOrderItem.ProductName, tblBulkOrderItem.ProductCode, tblBulkOrder.BulkOrderID, tblBulkOrderItem.Weight, 
                      tblBulkOrder.OrderDate, tblBulkOrderItem.Status, tblBulkOrderItem.ETA, tblBulkOrderItem.Location_MDB, tblBulkOrderItem.ItemNotes, 
                      tblBulkOrderItem.AmountReceived
FROM         tblBulkOrder INNER JOIN
                      tblBulkOrderItem ON tblBulkOrder.BulkOrderID = tblBulkOrderItem.BulkOrderID INNER JOIN
                      tblClient ON tblBulkOrder.ClientID = tblClient.ClientID LEFT OUTER JOIN
                      tblProductMaster RIGHT OUTER JOIN
                      tblDivision INNER JOIN
                      tblProductDetail ON tblDivision.DivisionID = tblProductDetail.DivisionID ON tblProductMaster.ProductMasterID = tblProductDetail.ProductMasterID ON 
                      tblBulkOrderItem.ProductMasterID = tblProductMaster.ProductMasterID
WHERE     (tblBulkOrderItem.Status = 'OP')
ORDER BY tblClient.ClientName, tblProductMaster.Division_MDB, Supplier</CommandText>
      </Query>
