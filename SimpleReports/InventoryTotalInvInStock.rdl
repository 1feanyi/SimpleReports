  <DataSets>
    <DataSet Name="Products">
      <Query>
        <DataSourceName>dtsrc_products</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Client">
            <Value>=Parameters!Client.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     tblClient.ClientID, tblClient.ClientName, tblDivision.DivisionName, tblDivision.BusinessUnit, tblProductMaster.MasterCode, tblProductMaster.MasterName, 
                      tblProductDetail.ProductCode, tblProductDetail.ProductName, tblBulk.LotNumber, tblStock.ShelfStatus AS Status, tblBulk.CurrentWeight AS Weight, 
                      tblBulk.Bin AS Location, tblStock.QtyOnHand AS Qty, tblShelfMaster.Size, tblBulk.ExpirationDate, tblBulk.CeaseShipDate, tblBulk.QCDate, tblBulk.ReturnLocation, 
                      tblProductMaster.SUPPLYID AS Supplier, tblBulk.ReceiveDate, tblBulk.MfgDate
FROM         tblProductDetail INNER JOIN
                      tblProductMaster ON tblProductDetail.ProductMasterID = tblProductMaster.ProductMasterID INNER JOIN
                      tblClient ON tblProductMaster.ClientID = tblClient.ClientID INNER JOIN
                      tblBulk ON tblProductMaster.ProductMasterID = tblBulk.ProductMasterID INNER JOIN
                      tblStock ON tblBulk.BulkID = tblStock.BulkID INNER JOIN
                      tblShelfMaster ON tblProductDetail.ProductDetailID = tblShelfMaster.ProductDetailID INNER JOIN
                      tblDivision ON tblProductDetail.DivisionID = tblDivision.DivisionID
WHERE     (tblStock.QtyOnHand &gt; 0) AND (tblClient.ClientID = @Client)
ORDER BY tblClient.ClientID, tblClient.ClientName, tblDivision.DivisionName, tblProductDetail.ProductName</CommandText>
      </Query>
