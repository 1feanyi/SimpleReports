  <DataSets>
    <DataSet Name="Products">
      <Query>
        <DataSourceName>dtsrc_products</DataSourceName>
        <CommandText>SELECT     tblClient.ClientID, tblClient.ClientCode, tblClient.ClientName, tblOrderMaster.Division_MDB, tblOrderMaster.OrderID, tblOrderMaster.OrderDate, 
                      tblOrderItem.ProductDetailID, tblProductDetail.ProductCode, tblProductDetail.ProductName, tblOrderItem.Qty, tblOrderItem.Size, tblOrderMaster.Company, 
                      tblOrderMaster.Country, tblOrderMaster.SpecialInternal, tblOrderMaster.Special, tblOrderItem.ShipDate, tblOrderItem.Warehouse
FROM         tblOrderMaster INNER JOIN
                      tblOrderItem ON tblOrderMaster.OrderID = tblOrderItem.OrderID INNER JOIN
                      tblClient ON tblOrderMaster.ClientID = tblClient.ClientID INNER JOIN
                      tblProductDetail ON tblOrderItem.ProductDetailID = tblProductDetail.ProductDetailID
WHERE     (tblOrderItem.Qty &gt; 0) AND (tblOrderItem.ShipDate IS NULL)
ORDER BY tblClient.ClientID, tblOrderMaster.Division_MDB, tblOrderMaster.OrderID</CommandText>
      </Query>
