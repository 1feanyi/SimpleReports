  <DataSets>
    <DataSet Name="Products">
      <Query>
        <DataSourceName>dtsrc_products</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Client">
            <Value>=Parameters!Client.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     tblClient.ClientID, tblClient.ClientName, tblDivision.DivisionName, tblDivision.BusinessUnit, tblProductDetail.ProductName, tblProductDetail.ProductCode, 
                      tblProductDetail.ProductDetailID, tblProductDetail.ProductMasterID, tblProductMaster.MasterCode, tblProductMaster.MasterName, tblBulk.ExpirationDate, 
                      tblBulk.CeaseShipDate, tblBulk.LotNumber, tblStock.QtyOnHand AS Quantity, tblShelfMaster.Size, tblBulk.CurrentWeight AS Weight, tblStock.StockID, 
                      tblProductMaster.SUPPLYID AS Supplier, tblProductMaster.ShlfLife AS [Shelf Life], tblBulk.Warehouse, tblBulk.BulkStatus
FROM         tblProductDetail INNER JOIN
                      tblProductMaster ON tblProductDetail.ProductMasterID = tblProductMaster.ProductMasterID INNER JOIN
                      tblClient ON tblProductMaster.ClientID = tblClient.ClientID INNER JOIN
                      tblBulk ON tblProductMaster.ProductMasterID = tblBulk.ProductMasterID INNER JOIN
                      tblDivision ON tblProductDetail.DivisionID = tblDivision.DivisionID LEFT OUTER JOIN
                      tblStock ON tblBulk.BulkID = tblStock.BulkID LEFT OUTER JOIN
                      tblShelfMaster ON tblStock.ShelfID = tblShelfMaster.ShelfID
WHERE     (tblStock.QtyOnHand &gt; 1) AND (tblClient.ClientID = @Client) AND (tblBulk.CeaseShipDate BETWEEN @StartDate AND @EndDate) OR
                      (tblClient.ClientID = @Client) AND (tblBulk.BulkStatus &lt;&gt; 'QC') AND (tblBulk.CeaseShipDate BETWEEN @StartDate AND @EndDate) OR
                      (tblClient.ClientID = @Client) AND (tblBulk.BulkStatus = 'WASTE') AND (tblBulk.CeaseShipDate BETWEEN @StartDate AND @EndDate)
ORDER BY tblClient.ClientID, tblClient.ClientName, tblDivision.DivisionName, tblProductDetail.ProductName</CommandText>
      </Query>
