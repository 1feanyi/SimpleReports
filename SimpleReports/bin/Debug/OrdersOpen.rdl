  <DataSets>
    <DataSet Name="Products">
      <Query>
        <DataSourceName>dtsrc_products</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Client">
            <Value>=Parameters!Client.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     tblClient.ClientID, tblClient.ClientCode, tblClient.ClientName, tblDivision.DivisionName, tblOrderMaster.OrderID, tblOrderMaster.OrderDate, 
                      tblOrderItem.ProductDetailID, tblProductDetail.ProductCode, tblProductDetail.ProductName, tblOrderItem.Qty, tblOrderItem.Size, tblOrderMaster.Company, 
                      tblOrderMaster.Country, tblOrderMaster.SpecialInternal, tblOrderMaster.Special, tblOrderItem.ShipDate, tblOrderItem.Warehouse, tblOrderMaster.Req, 
                      tblOrderMaster.SalesRep, tblOrderMaster.SalesEmail, tblOrderMaster.ReqEmail, tblOrderItem.Status
FROM         tblOrderMaster INNER JOIN
                      tblOrderItem ON tblOrderMaster.OrderID = tblOrderItem.OrderID INNER JOIN
                      tblClient ON tblOrderMaster.ClientID = tblClient.ClientID INNER JOIN
                      tblProductDetail ON tblOrderItem.ProductDetailID = tblProductDetail.ProductDetailID INNER JOIN
                      tblDivision ON tblProductDetail.DivisionID = tblDivision.DivisionID
WHERE     (tblOrderItem.Qty &gt; 0) AND (tblOrderItem.ShipDate IS NULL) AND (tblClient.ClientID = @Client)
ORDER BY tblClient.ClientName, tblDivision.DivisionName, tblOrderMaster.OrderID</CommandText>
      </Query>
