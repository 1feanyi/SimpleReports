  <DataSets>
    <DataSet Name="Products">
      <Query>
        <DataSourceName>dtsrc_products</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Client">
            <Value>=Parameters!Client.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     tblClient.ClientID, tblClient.ClientName, tblDivision.DivisionName, tblDivision.BusinessUnit, tblProductMaster.ProductMasterID, tblProductMaster.MasterCode, 
                      tblProductMaster.MasterName, tblProductDetail.ProductDetailID, tblProductDetail.ProductCode, tblProductDetail.ProductName, tblShelfMaster.ShelfID, 
                      tblShelfMaster.Size, tblShelfMaster.Discontinued
FROM         tblProductDetail LEFT OUTER JOIN
                      tblProductMaster ON tblProductDetail.ProductMasterID = tblProductMaster.ProductMasterID INNER JOIN
                      tblClient ON tblProductMaster.ClientID = tblClient.ClientID INNER JOIN
                      tblShelfMaster ON tblProductDetail.ProductDetailID = tblShelfMaster.ProductDetailID INNER JOIN
                      tblDivision ON tblProductDetail.DivisionID = tblDivision.DivisionID
WHERE     (tblClient.ClientID = @Client)
ORDER BY tblClient.ClientName, tblDivision.DivisionName, tblProductDetail.ProductCode, tblShelfMaster.Size</CommandText>
      </Query>
      <Fields>
        <Field Name="ClientID">
          <DataField>ClientID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ClientName">
          <DataField>ClientName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DivisionName">
