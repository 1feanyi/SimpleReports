  <DataSets>
    <DataSet Name="Products">
      <Query>
        <DataSourceName>dtsrc_products</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Client">
            <Value>=Parameters!Client.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     tblClient.ClientID, tblClient.ClientName, tblDivision.DivisionID, tblDivision.DivisionName, tblDivision.BusinessUnit, tblProductMaster.ProductMasterID, 
                      tblProductMaster.MasterCode, tblProductMaster.MasterName, tblProductDetail.ProductDetailID, tblProductDetail.ProductCode, tblProductDetail.ProductName, 
                      tblProductDetail.CreateDate AS CreateDate_dtl, tblProductDetail.UpdateDate AS UpdateDate_dtl, tblProductMaster.SUPPLYID, tblProductMaster.CreateDate, 
                      tblProductMaster.UpdateDate, tblProductMaster.ProductSetupDate, tblProductMaster.ShlfLife, tblProductMaster.DENSITY, tblShelfMaster.Size, 
                      tblShelfMaster.UnitWeight, tblShelfMaster.ShelfID
FROM         tblProductDetail LEFT OUTER JOIN
                      tblProductMaster ON tblProductDetail.ProductMasterID = tblProductMaster.ProductMasterID INNER JOIN
                      tblClient ON tblProductMaster.ClientID = tblClient.ClientID INNER JOIN
                      tblShelfMaster ON tblProductDetail.ProductDetailID = tblShelfMaster.ProductDetailID INNER JOIN
                      tblDivision ON tblProductDetail.DivisionID = tblDivision.DivisionID
WHERE     (tblClient.ClientID = @Client)
ORDER BY tblClient.ClientID, tblClient.ClientName, tblDivision.DivisionName, tblProductMaster.MasterCode</CommandText>
      </Query>
