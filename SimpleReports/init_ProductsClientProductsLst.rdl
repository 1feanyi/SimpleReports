  <DataSets>
    <DataSet Name="ProductsList">
      <Query>
        <DataSourceName>dtsrc_products</DataSourceName>
        <CommandText>SELECT DISTINCT tblClient.ClientID, tblClient.ClientName, tblProductDetail.ProductCode, tblProductDetail.ProductName, tblProductDetail.Division_MDB
FROM         tblProductDetail INNER JOIN
                      tblProductMaster ON tblProductDetail.ProductMasterID = tblProductMaster.ProductMasterID INNER JOIN
                      tblClient ON tblProductMaster.ClientID = tblClient.ClientID
ORDER BY tblClient.ClientID, tblClient.ClientName, tblProductDetail.ProductCode, tblProductDetail.ProductName</CommandText>
      </Query>
      <Fields>
        <Field Name="ClientID">
          <DataField>ClientID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ClientName">
          <DataField>ClientName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProductCode">
          <DataField>ProductCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProductName">
          <DataField>ProductName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Division_MDB">
          <DataField>Division_MDB</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!ClientID.Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Parameters!Client.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
    <DataSet Name="Clients">
