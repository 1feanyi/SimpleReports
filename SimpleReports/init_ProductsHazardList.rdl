  <DataSets>
    <DataSet Name="Products">
      <Query>
        <DataSourceName>dtsrc_products</DataSourceName>
        <CommandText>SELECT DISTINCT 
                      tblClient.ClientID, tblClient.ClientName, tblProductDetail.ProductCode, tblProductDetail.ProductName, tblProductDetail.Division_MDB, 
                      tblProductDetail.GRNUNNUMBER, tblProductDetail.GRNPKGRP, tblProductDetail.GRNHAZCL, tblProductDetail.AIRUNNUMBER, tblProductDetail.AIRPKGRP, 
                      tblProductDetail.AIRHAZCL
FROM         tblProductDetail INNER JOIN
                      tblProductMaster ON tblProductDetail.ProductMasterID = tblProductMaster.ProductMasterID INNER JOIN
                      tblClient ON tblProductMaster.ClientID = tblClient.ClientID
ORDER BY tblClient.ClientID, tblClient.ClientName, tblProductDetail.ProductCode, tblProductDetail.ProductName</CommandText>
      </Query>
      <Fields>
        <Field Name="ClientID">
          <DataField>ClientID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
